{
	"version": 2,
	"builds": [
		{
			"src": "index.php",
			"use": "vercel-php@0.7.0",
			"config": {
				"includeFiles": [
					"**",
					"img/**",
					"css/**",
					"js/**",
					"helpers/**"
				]
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.php"
		},
		{
			"src": "/(favicon.ico)"
		}
	]
}